<?xml version="1.0"?>
<!-- 
  Atom Robot XACRO Description
  This file defines the Atom humanoid robot for Gazebo simulation
  Based on the original atom.urdf file
-->
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="atom">

  <!-- Include Gazebo plugins and materials -->
  <xacro:include filename="$(find atom_urdf)/urdf/atom_gazebo.xacro" />
  <xacro:include filename="$(find atom_urdf)/urdf/atom_materials.xacro" />

  <!-- Robot Parameters -->
  <xacro:property name="PI" value="3.1415926535897931" />
  <xacro:property name="mesh_path" value="package://atom_urdf/meshes" />
  
  <!-- Material Properties -->
  <xacro:property name="default_mass" value="1.0" />
  <xacro:property name="default_inertia" value="0.001" />

  <!-- Macro for creating inertial properties -->
  <xacro:macro name="default_inertial" params="mass">
    <inertial>
      <mass value="${mass}" />
      <inertia ixx="${default_inertia}" ixy="0.0" ixz="0.0"
               iyy="${default_inertia}" iyz="0.0"
               izz="${default_inertia}" />
    </inertial>
  </xacro:macro>

  <!-- World Link -->
  <link name="world"/>
  
  <!-- Base Link (World Reference) -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.001 0.001 0.001"/>
      </geometry>
    </visual>
  </link>

  <!-- World to Base Joint -->
  <joint name="world_joint" type="fixed">
    <origin xyz="0 0 1.2" rpy="0 0 0" />
    <parent link="world" />
    <child link="base_link" />
  </joint>

  <!-- Pelvis Link -->
