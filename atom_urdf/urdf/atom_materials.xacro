<?xml version="1.0"?>
<!-- 
  Atom Robot Materials Definition
  This file defines visual materials for the Atom robot
-->
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- White Material -->
  <material name="white">
    <color rgba="0.9 0.9 0.9 1.0"/>
  </material>

  <!-- Black Material -->
  <material name="black">
    <color rgba="0.1 0.1 0.1 1.0"/>
  </material>

  <!-- Red Material -->
  <material name="red">
    <color rgba="0.8 0.2 0.2 1.0"/>
  </material>

  <!-- Blue Material -->
  <material name="blue">
    <color rgba="0.2 0.2 0.8 1.0"/>
  </material>

  <!-- Green Material -->
  <material name="green">
    <color rgba="0.2 0.8 0.2 1.0"/>
  </material>

  <!-- Gray Material -->
  <material name="gray">
    <color rgba="0.5 0.5 0.5 1.0"/>
  </material>

  <!-- Orange Material -->
  <material name="orange">
    <color rgba="1.0 0.5 0.0 1.0"/>
  </material>

  <!-- Yellow Material -->
  <material name="yellow">
    <color rgba="1.0 1.0 0.0 1.0"/>
  </material>

  <!-- Silver Material -->
  <material name="silver">
    <color rgba="0.7 0.7 0.7 1.0"/>
  </material>

  <!-- Dark Gray Material -->
  <material name="dark_gray">
    <color rgba="0.3 0.3 0.3 1.0"/>
  </material>

</robot>